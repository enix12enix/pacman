<%= javascript_tag do -%>
      google.load('visualization', '1.0', {'packages':['corechart']});

      google.setOnLoadCallback(drawChart);

      function drawChart() {
        var data = new google.visualization.DataTable();
        data.addColumn('string', 'Topping');
        data.addColumn('number', 'Slices');
        data.addRows([
          ['Pass', <%=latest_ver[0]%>],
          ['Fail', <%=latest_ver[1]%>],
          ['Pending', <%=latest_ver[2]%>]
        ]);

        var options = {'title':"Latest Build <%=env%>-<%=latest_ver[3]%> Test Status",
                       'width':400,
                       'height':300,
                       'colors':['#109618', '#dc3912', '#ff9900']};

        var chart = new google.visualization.PieChart(document.getElementById("<%=env%>_pie_chart_div"));
        chart.draw(data, options);
      }
<%- end -%>
